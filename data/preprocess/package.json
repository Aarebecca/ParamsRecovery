{
  "name": "preprocess",
  "version": "1.0.0",
  "description": "Preprocess program for dealing with js source code",
  "main": "index.ts",
  "scripts": {
    "test": "jest",
    "test-live": "cross-env DEBUG_MODE=1 jest --watch ./__tests__/unit/utils/walk.spec.ts",
    "build": "npm run clean && tsc",
    "exec": "node --expose-gc --max-old-space-size=61440 build/src/index.js",
    "launch": "npm run build && npm run exec",
    "launch-test": "npm run launch uglify 20 100 1 dev",
    "clean": "rimraf build && mkdir build"
  },
  "author": "Aaron",
  "license": "ISC",
  "dependencies": {
    "@antv/util": "^2.0.17",
    "@babel/generator": "^7.16.0",
    "@babel/parser": "^7.16.0",
    "@babel/traverse": "^7.16.0",
    "colors": "^1.4.0",
    "lodash": "^4.17.21",
    "prettier": "^2.4.1",
    "uglify-js": "^3.14.2"
  },
  "devDependencies": {
    "@babel/types": "^7.16.0",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.6",
    "@types/uglify-js": "^3.13.1",
    "cross-env": "^7.0.3",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}
